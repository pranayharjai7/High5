<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="700.0" prefWidth="900.0" styleClass="backgroundForPrimaryFxml" stylesheets="@../styles/Modification.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Survey5.controller.SavedSurveysController">
   <center>
      <ScrollPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <content>
            <AnchorPane fx:id="MySurveysAnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="backgroundForPrimaryFxml" stylesheets="@../styles/Modification.css">
               <children>
                  <Label layoutX="275.0" layoutY="26.0" text="My Surveys">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <TextField fx:id="searchBarField" layoutX="229.0" layoutY="87.0" promptText="Enter Keywords here" styleClass="textBoxBorders" stylesheets="@../styles/Modification.css" />
                  <Button fx:id="searchButton" layoutX="399.0" layoutY="87.0" mnemonicParsing="false" onAction="#searchButtonClicked" style="-fx-background-color: #5e7b89;" styleClass="buttonBorders" stylesheets="@../styles/Modification.css" text="Search" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
   </center>
   <top>
      <AnchorPane prefHeight="125.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
             <Button fx:id="backButton" layoutX="27.0" layoutY="16.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="32.0" prefWidth="50.0" styleClass="addBackButtonPic" stylesheets="@../styles/Modification.css" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0" />
         </children>
      </AnchorPane>
   </top>
   <left>
      <AnchorPane prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <AnchorPane prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <AnchorPane prefHeight="125.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
