<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="newSurveyAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Survey5.controller.CreateSurveyController">
    <children>
        <Label fx:id="logoLabel" layoutX="230.0" layoutY="34.0" text="SURVEY 5">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
        <TextField fx:id="nameField" layoutX="230.0" layoutY="187.0" prefHeight="26.0" prefWidth="204.0" promptText="Enter Name" />
        <Button fx:id="OKButton" layoutX="230.0" layoutY="234.0" mnemonicParsing="false" onAction="#OKButtonClicked" text="OK" />
        <Label fx:id="nameOfSurveyLabel" layoutX="53.0" layoutY="185.0" text="Name Of Survey:">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
